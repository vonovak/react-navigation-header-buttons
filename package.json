  "author": "Vojtech Novak <vonovak@gmail.com> (https://github.com/vonovak)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/vonovak/react-navigation-header-buttons/issues"
  },
  "homepage": "https://github.com/vonovak/react-navigation-header-buttons#readme",
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^17.0.2",
    "@evilmartians/lefthook": "^1.2.2",
    "@react-native/eslint-config": "^0.72.2",
    "@react-navigation/elements": "^1.3.17",
    "@react-navigation/native": "^6.1.6",
    "@release-it/conventional-changelog": "^5.0.0",
    "@testing-library/react-native": "^12.4.3",
    "@types/jest": "^29.5.1",
    "@types/react": "~18.2.11",
    "commitlint": "^17.0.2",
    "del-cli": "^5.0.0",
    "eslint": "^8.51.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "^29.5.0",
    "pod-install": "^0.1.0",
    "prettier": "^2.0.5",
    "react": "18.2.0",
    "react-native": "0.72.2",
    "react-native-builder-bob": "^0.23.2",
    "react-native-safe-area-context": "^4.5.3",
    "react-test-renderer": "^18.2.0",
    "release-it": "^15.0.0",
    "typescript": "^5.1.3"
  },
  "peerDependencies": {
    "@react-navigation/elements": ">=1",
    "@react-navigation/native": ">=6",
    "react": "*",
    "react-native": ">=0.72.0"
  },
  "engines": {
    "node": ">= 20.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "release-it": {
    "git": {
      "commitMessage": "chore: release ${version}",
      "tagName": "v${version}"
    },
    "npm": {
      "publish": true
    },
    "github": {
      "release": true
    },
    "plugins": {
      "@release-it/conventional-changelog": {
        "preset": "angular"
      }
    }
  },
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "dependencies": {
    "react-to-imperative": "^0.2.0"
  }
}
